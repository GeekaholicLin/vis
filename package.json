    "globOptions": {
      "matchBase": false,
      "nonegate": true
    },
    "linters": {
      "src/**/!(*.min).{js,jsx,css,scss,json}": ["prettier --write", "git add"],
      "!(manifest).{js,json}": ["prettier --write", "git add"]
    }
